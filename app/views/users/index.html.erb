<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>User type</th>
      <th>Email</th>
      <th>Phone Numbers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.cpf %></td>
        <td><%= user.user_type %></td>
        <td><%= user.email %></td>
        <td><%= user.phone_number.map { |x| x.phone_number }.join(',').html_safe %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Voltar', homepage_path %>
<br>
